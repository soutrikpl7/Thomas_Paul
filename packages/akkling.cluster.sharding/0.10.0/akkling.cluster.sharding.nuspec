<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Akkling.Cluster.Sharding</id>
    <version>0.10.0</version>
    <authors>Bartosz Sypytkowski</authors>
    <owners>Bartosz Sypytkowski</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/Horusiath/Akkling/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Horusiath/Akkling</projectUrl>
    <description>F# wrapper library for Akka.NET cluster sharding module.</description>
    <releaseNotes>* Upgraded Akka.NET dependencies to 1.4.2
      * Consolidated supported .NET version to .NET Standard 2.0
      * Removed PersistentView (as its no longer supported)
      * Akkling.Streams: added support to stream refs, observables, cancellation tokens and withContext variants.
      * Akkling.Streams: switched tuple args to value tuples to match Akka.Streams API.</releaseNotes>
    <copyright>Bartosz Sypytkowski</copyright>
    <tags>akka.net fsharp cluster sharding</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Akkling.DistributedData" version="0.10.0" exclude="Build,Analyzers" />
        <dependency id="Akkling.Persistence" version="0.10.0" exclude="Build,Analyzers" />
        <dependency id="Akkling" version="0.10.0" exclude="Build,Analyzers" />
        <dependency id="Akka.Cluster.Sharding" version="1.4.2" exclude="Build,Analyzers" />
        <dependency id="FSharp.Core" version="4.7.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>